<!--addRecord.wxml-->


<view class="container">

  <view class="doc-title">Upload Form</view>

  <view class="zan-form">
    <view class="zan-cell zan-form__item">
      <text class="zan-form__title">Blood Sugar</text>
      <input
        class="zan-form__input zan-cell__bd"
        type="digit"
        placeholder="Upload your Blood Sugar Here"
      />
    </view>
    <view class="zan-cell zan-form__item">
      <text class="zan-form__title">Blood Pressure</text>
      <view class=".zan-form__textarea textarea">
        <input
          class="zan-form__input zan-cell__bd"
          type="number"
          maxlength="3"
          placeholder="High"
        />
      </view>
      <view class=".zan-form__textarea textarea">
        <input
          class="zan-form__input zan-cell__bd"
          type="number"
          maxlength="3"
          placeholder="Low"
        />
      </view>
    </view>
    <view class="zan-cell zan-form__item zan-cell--last-child">
      <text class="zan-form__title">Blood Fat</text>
      <view class="zan-form__input zan-cell__bd">
        <input
          class="zan-form__input zan-cell__bd"
          type="digit"
          placeholder="Upload your Blood Fat Here"
        />
      </view>
    </view>
  </view>
  <view class="zan-panel">
    <view class="zan-btns">
      <button class="zan-btn zan-btn--primary">Upload</button>
    </view>
  </view>
</view>

